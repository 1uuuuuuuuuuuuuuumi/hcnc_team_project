package sample.service;

import java.util.HashMap;
import java.util.List;
import java.util.Map;

import javax.annotation.Resource;

import org.springframework.beans.factory.annotation.Autowired;
import org.springframework.stereotype.Service;

import admin.mapper.MainMapper;
import admin.mapper.TestMapper;

@Service
public class TestService {
	@Autowired
	private TestMapper testMapper;
	
    public List<Map<String, Object>> selectUserT(Map<String, Object> param){
    	return testMapper.selectUserT(param);
    };
    
    public List<Map<String, Object>> selectGrade(){
    	return testMapper.selectGrade();
    };
    
    public List<Map<String, Object>> selectProductT(Map<String, Object> param){
    	return testMapper.selectProductT(param);
    };
    
    public List<Map<String, Object>> selectCategoryT(){
    	return testMapper.selectCategoryT();
    };
    
    public List<Map<String, Object>> selectSubCategoryT(){
    	return testMapper.selectSubCategoryT();
    };
    
    public List<Map<String, Object>> selectProductCal(){
    	return testMapper.selectProductCal();
    };
    
    public List<Map<String, Object>> selectUserCal(){
    	return testMapper.selectUserCal();
    };
    
    
    public List<Map<String, Object>> selectOrderT(Map<String, Object> param){
    	return testMapper.selectOrderT(param);
    };
	
}
