<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="user.mapper.UserMapper">
	<select id="selectGradeList" resultType="java.util.Map">
		SELECT GRADE_CODE
			, GRADE_NAME
			, DISCOUNT_RATE
			, STANDARD_AMOUNT
		  FROM GRADES;
	</select>
</mapper>